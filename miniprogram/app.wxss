@import 'weui.wxss';
/* 导入微信官方wxss */
page {
  background-color: #f5f5f5;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* 公用列表头 */
.doc-container{
  overflow: hidden;
  min-height: 100vh;
  box-sizing: border-box;
}
.doc-title{
  position: relative;
  margin: 10px 15px;
  line-height: 25px;
  font-size: 25px;
  color:#666;
}